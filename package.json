{
  "name": "evolution-api-sdk",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:real": "jest tests/integration/real-api.test.ts",
    "test:real:complete": "jest tests/integration/complete-api.test.ts",
    "test:integration": "jest tests/integration/integration-test.test.ts",
    "test:groups": "jest tests/integration/api-group.test.ts",
    "test:groups:improved": "jest tests/integration/improved-api-group.test.ts",
    "test:websocket": "jest tests/integration/websocket-events.test.ts",
    "test:websocket:improved": "jest tests/integration/improved-websocket-events.test.ts",
    "test:all-integration": "jest \"tests/integration/*.test.ts\"",
    "test:real:interactive": "node scripts/run-real-tests.js",
    "run:complete-tests": "node scripts/run-complete-tests.js",
    "setup:real-tests": "node scripts/setup-real-tests.js",
    "test:connection": "node scripts/test-connection.js",
    "check:test-files": "node scripts/check-test-files.js",
    "check:env": "node scripts/check-test-env.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "npm run build",
    "publish:npm": "node scripts/publish.js",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "whatsapp",
    "api",
    "evolution-api",
    "sdk",
    "whatsapp-api"
  ],
  "author": "Agência FER",
  "license": "MIT",
  "description": "Um SDK profissional para integração com a Evolution API",
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "amqplib": "^0.10.8",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "ws": "^8.18.2"
  }
}
